;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	core1.home. admin.core1.home. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

; name servers - NS records
		IN		NS		core1.home.
		IN		NS		core2.home.

;	PTR records
{% for line in clients_reserved %}
13		IN		PTR		{{line.hostname}}.{{dns_intzone}}.		;		{{line.IP}}
{% endfor %}